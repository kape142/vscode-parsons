            "text": "import java.util.ArrayList;\r\nimport java.util.List;\r\n\r\npublic class Stock {\r\n    \r\n    private final String ticker;\r\n    private double price;\r\n    private List<StockListener> listeners = new ArrayList<StockListener>();\r\n    \r\n    public Stock(String ticker, double price) {\r\n        this.ticker = ticker;\r\n        this.price = price;\r\n    }\r\n\r\n    public double getPrice() {\r\n        return this.price;\r\n    }\r\n\r\n    public void setPrice(double newPrice) {\r\n        double oldPrice = this.price;\r\n        this.price = newPrice;\r\n        if (this.price != oldPrice) {\r\n            firePriceChanged(oldPrice);\r\n            /*$parson{\"id\":\"eHQePWNQmNki\",\"text\":\"firePriceChanged(oldPrice)\"}*/\r\n        }\r\n    }\r\n    \r\n    public void addStockListener(StockListener listener) {\r\n        listeners.add(listener);\r\n        /*$parson{\"id\":\"nkdFXCgLTlrx\",\"text\":\"listeners.add(listener)\"}*/\r\n    }\r\n    \r\n    protected void firePriceChanged(double oldPrice) {\r\n        for (StockListener listener : listeners) {\r\n            listener.stockPriceChanged(this, oldPrice, this.price);\r\n            /*$parson{\"id\":\"vinjcLTCoiAl\",\"text\":\"this\"}*/\r\n            /*$parson{\"id\":\"SyejIzwQuOVu\",\"text\":\"oldPrice\"}*/\r\n            /*$parson{\"id\":\"ndFLzbplckkJ\",\"text\":\"this.price\"}*/\r\n        }\r\n    }\r\n}\r\n",
